<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9"
        xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">
  <% @pages.each do |page| %>
  <url>
    <loc><%= request.base_url + page[:url] %></loc>
    <lastmod><%= page[:lastmod] %></lastmod>
    <changefreq><%= page[:changefreq] %></changefreq>
    <priority><%= page[:priority] %></priority>
    <image:image>
      <image:loc><%= request.base_url %>/icon.png</image:loc>
      <image:title>Élagage Dubois - États des lieux professionnels</image:title>
      <image:caption>Service professionnel d'états des lieux en Île-de-France</image:caption>
    </image:image>
  </url>
  <% end %>
</urlset>
